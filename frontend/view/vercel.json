{
    "version": 2,
    "projects": {
      "frontend": {
        "src": "app/",
        "use": "@vercel/static-build",
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      },
      "backend": {
        "src": "../../",
        "use": "@vercel/node"
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" }, 
      { "src": "/(.*)", "dest": "app/$1" }
    ]
  }
  
{
  "version": 2,
  "builds": [
    {
      "src": "frontend/view/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/frontend/view/.next/$1" }
  ]
}
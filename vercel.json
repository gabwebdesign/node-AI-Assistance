{
  "version": 2,
  "builds": [
    {
      "src": "frontend/view/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "/index.js" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/.next/server/app/page.js"
    }
  ]
}